@using Extrade.ViewModels;
@model PaginingViewModel<List<MarketerViewModels>>

    <!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="~/Css/Markter/Markter.css"/>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.min.js" integrity="sha384-QJHtvGhmr9XOIpI6YVutG+2QOK9T+ZnN4kzFN1RtK3zEFEIsxhlmWl5/YESvpZ13" crossorigin="anonymous"></script>
    <script defer src="https://use.fontawesome.com/releases/v5.15.4/js/all.js" integrity="sha384-rOA1PnstxnOBLzCLMcre8ybwbTmemjzdNlILg8O7z1lUkLXozs4DHonlDtnE7fpc" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.5.0/Chart.min.js"></script>
    <title>USer</title>
</head>
<body>
<div class="d-flex all ">
  <div class="border border-3 SideBar col-2">
        <div class="d-flex mt-3">
            <div>
              <img class="image" src="setting.png"/>
            </div>
            <div>
              <select class="select">
                <option value="volvo">Flourish</option>
                <option value="saab">Saab</option>
                <option value="opel">Opel</option>
                <option value="audi">Audi</option>
              </select>
              <p class="textobacity">More User Details</p>
            </div>
        </div>
        <div class="d-flex mt-2">
           <img class="image" src="dashboard.png"/>
        <div>
           <p class="select">Dashboard</p>
        </div>
        </div>
        <p class="title">DEDRIJF</p>
        <div class="d-flex mt-1">   
          <i class="icon fa fa-thin fa-user" ></i>
        <div>
          <p class="select">Werknemers</p>
        </div>
        </div>
        <div class="d-flex mt-2">
          <i class="icon fa fa-solid fa-hand-holding-medical"></i>
        <div>
           <p class="select">Werknemers</p>
        </div>
        </div>

        <div class="d-flex mt-2 calanderdiv">
            <i class="calandericon fa fa-solid fa-calendar-day"></i>
        <div>
            <p class="select"> Verlof</p>
        </div>
        </div>
        <p class="title">DEDRIJF</p>

        <div class="d-flex mt-1">   
            <i class="icon fa fa-solid fa-receipt"></i>
          <div>
            <p class="select">Werknemers</p>
          </div>
        </div>
        <div class="d-flex mt-1">   
            <i class="icon  fa fa-solid fa-file"></i>
          <div>
            <p class="select">Documents</p>
          </div>
        </div>
        <div class="d-flex mt-1">   
            <i class="icon fa fa-regular fa-wrench"></i>
          <div>
            <p class="select">Tools</p>
          </div>
        </div>  
           <p class="title">DEDRIJF</p>
        <div class="d-flex mt-1">   
          <i class="icon fa fa-regular fa-question"></i>
          <div>
            <p class="select">FAQ</p>
          </div>
        </div> 
        <div class="d-flex mt-1">   
          <i class=" icon fa fa-regular fa-headphones"></i>
          <div>
            <p class="select">Services</p>
          </div>
        </div> 
       
  </div> 
  <div class="col-10">

    <div class="input-group mb-3">
        <span class="input-group-text" id="basic-addon1"><i class="fa fa-light fa-magnifying-glass-arrow-right"></i></span>
        <input type="text" class="form-control" placeholder="Username" aria-label="Username" aria-describedby="basic-addon1">
        <span class="input-group-text" id="basic-addon2"><i class="fa fa-light fa-bell"></i></span>
      </div>
      <div class="d-flex">
      <h1 class="mt-5 ms-5 ">Markter</h1>
      <a class="plusicon"><i class="fa fa-solid fa-plus"></i></a>
    </div>
        
        <div class="border-bottom"></div> 
        <div class="ms-5 me-5">
        <table table class="table table-striped table-hover mt-5 ">
            <thead>
              <tr>
               
                <th scope="col">User ID</th>
                <th scope="col">Tax Card</th>
                <th scope="col">Collection Name</th>
                <th scope="col">Controller in Collection</th>
              </tr>
            </thead>
            <tbody>
              @foreach (var m in Model.Data)
               {
              <tr>
                
                <td>@m.UserID</td>
                <td>@m.TaxCard</td>
                <td><a asp-action="Get" asp-controller="Product">Add Product</a></td>
              </tr>
               }
            </tbody>
          </table>
        </div> 
          

</div>
</div>
</body>
</html>